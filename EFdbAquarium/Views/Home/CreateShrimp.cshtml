@model EFdbAquarium.Models.Shrimp
@{
    ViewBag.Title = "Додавання креветки";
}
<form asp-action="createshrimp" asp-controller="home">
    <div class="form-group">
        <label asp-for="Type" class="control-label">Вид</label>
        <input type="text" asp-for="Type" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Length" class="control-label">Довжина</label>
        <input type="text" asp-for="Length" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Price" class="control-label">Ціна</label>
        <input type="text" asp-for="Price" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Надіслати" class="btn btn-default" />
    </div>
</form>